####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/bin/" || $(MKDIR) "$(INSTALL_ROOT)/usr/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/bin/"


install_startupfiles: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/" || $(MKDIR) "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/sdr-shell.sh" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/start-audigy.sh" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/start-delta44.sh" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/start_fifi.sh" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/start_rx" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"
	-$(INSTALL_FILE) "startup/start_softrock_lite.sh" "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"


uninstall_startupfiles: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/sdr-shell.sh"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/start-audigy.sh"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/start-delta44.sh"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/start_fifi.sh"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/start_rx"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/start_softrock_lite.sh"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/share/doc/sdr-shell/startup/"


install: install_target install_startupfiles  

uninstall: uninstall_target uninstall_startupfiles  

